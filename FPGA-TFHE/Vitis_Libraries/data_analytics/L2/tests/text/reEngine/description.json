{
    "gui": false, 
    "name": "Xilinx reEngine Test", 
    "description": "Xilinx Regex Engine Test", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u200",
        "u250", 
        "u50",
        "aws-vu9p-f1"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u200": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": []
                }
            }, 
            "containers": [
                {
                    "ldclflags": "--config PROJECT/conn_u200.cfg --config PROJECT/opts.cfg", 
                    "name": "reEngineKernel"
                }
            ]
        }, 
        "u250": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": []
                }
            }, 
            "containers": [
                {
                    "ldclflags": "--config PROJECT/conn_u250.cfg", 
                    "name": "reEngineKernel"
                }
            ]
        }, 
        "u50": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": []
                }
            }, 
            "containers": [
                {
                    "ldclflags": "--config PROJECT/conn_u50.cfg", 
                    "name": "reEngineKernel"
                }
            ]
        }
    }, 
    "launch": [
        {
            "name": "generic launch for sw_emu flows", 
            "target": "sw_emu", 
            "cmd_args": " -xclbin BUILD/reEngineKernel.xclbin -ln 5000 -dir PROJECT/log_data", 
            "ld_library_path": [
                "PROJECT/re_compile/lib/lib" 
            ]
        }, 
        {
            "name": "generic launch for hw_emu flows", 
            "target": "hw_emu", 
            "cmd_args": " -xclbin BUILD/reEngineKernel.xclbin -ln 1000 -dir PROJECT/log_data", 
            "ld_library_path": [
                "PROJECT/re_compile/lib/lib" 
            ]
        }, 
        {
            "name": "generic launch for hw flows", 
            "target": "hw", 
            "cmd_args": " -xclbin BUILD/reEngineKernel.xclbin -ln 5000 -dir PROJECT/log_data", 
            "ld_library_path": [
                "PROJECT/re_compile/lib/lib" 
            ]
        }
    ], 
    "pre_build": [
        {
	    "build_cmd": "make -C PROJECT/re_compile"
	}
    ], 
    "host": {
        "host_exe": "test.exe", 
        "compiler": {
            "sources": [
                "host/test.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "PROJECT/host", 
                "PROJECT/re_compile/lib/include", 
                "LIB_DIR/L1/include/sw", 
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/../utils/L1/include"
            ], 
            "options": "-O3 "
        }, 
        "linker": {
            "librarypaths": [
                "PROJECT/re_compile/lib/lib"
            ], 
            "libraries": [
                "onig", 
                "xfcompile"
            ], 
            "options": ""
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw", 
                "LIB_DIR/L2/include/hw", 
                "PROJECT/kernel"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "PROJECT/kernel/re_engine_kernel.cpp", 
                    "frequency": 300, 
                    "name": "reEngineKernel", 
                    "num_compute_units": 1
                }
            ], 
            "frequency": 300, 
            "name": "reEngineKernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 81920, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 1200, 
                    "vitis_hw_emu": 470, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
