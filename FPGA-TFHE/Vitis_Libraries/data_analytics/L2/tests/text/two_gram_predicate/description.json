{
    "gui": false,
    "name": "Xilinx Two Gram Predicate Test", 
    "description": "2-Gram Predicate based on inverted index", 
    "flow": "vitis", 
    "platform_type": "", 
    "platform_whitelist": [
        "u50", 
        "u200", 
        "u250", 
        "aws-vu9p-f1"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u50": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u50.cfg"
                    ]
                }
            }
        }, 
        "u200": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u200.cfg"
                    ]
                }
            }
        }, 
        "u250": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u250.cfg"
                    ]
                }
            }
        }, 
        "vck190": {
            "host": {
                "linker": {
                    "librarypaths": [
                        "$(SYSROOT)/usr/lib", 
                        "${SYSROOT}/opt/xilinx/xrt/lib"
                    ]
                }, 
                "compiler": {
                    "includepaths": [
                        "$(SYSROOT)/usr/include"
                    ], 
                    "options": "--sysroot=$(SYSROOT)"
                }
            }
        }
    }, 
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/TGP_Kernel.xclbin -in PROJECT/data/test.txt -golden PROJECT/data/golden.txt", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "host/main.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "host", 
                "kernel", 
                "LIB_DIR/L2/include/hw", 
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/../utils/L1/include"
            ], 
            "options": "-O3 "
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include/hw", 
                "kernel"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/predicate_kernel.cpp", 
                    "frequency": 300.0, 
                    "name": "TGP_Kernel", 
                    "num_compute_units": 1, 
                    "compute_units": [
                        {
                            "name": "TGP_Kernel", 
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "field", 
                                    "memory": "HBM[0]"
                                }, 
                                {
                                    "name": "offset", 
                                    "memory": "HBM[1]"
                                }, 
                                {
                                    "name": "idfValue", 
                                    "memory": "HBM[2]"
                                }, 
                                {
                                    "name": "tfAddr", 
                                    "memory": "HBM[3]"
                                }, 
                                {
                                    "name": "tfValue", 
                                    "memory": "HBM[4]"
                                }, 
                                {
                                    "name": "indexID", 
                                    "memory": "HBM[5]"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "frequency": 300.0, 
            "name": "TGP_Kernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": 300
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
