{
    "name": "Xilinx Hash Semi Join Benchmark",
    "description": "Xilinx Hash Semi Join Benchmark",
    "flow": "vitis",
    "gui": true,
    "platform_type": "pcie",
    "platform_whitelist": [
        "xilinx_u280_xdma_201920_3",
        "xilinx_u50_gen3x16_xdma_201920_3"
    ],
    "platform_blacklist": [
        "zc"
    ],
    "platform_properties": {
        "u280": {
            "containers": [
                {
                    "name": "semi_join",
                    "ldclflags": "--config PROJECT/conn_u280.cfg"
                }
            ]
        },
        "u50": {
            "containers": [
                {
                    "name": "semi_join",
                    "ldclflags": "--config PROJECT/conn_u50.cfg"
                }
            ]
        }
    },
    "launch": [
        {
            "name": "generic launch for sw_emu flows",
            "target": "sw_emu",
            "cmd_args": " -xclbin BUILD/semi_join.xclbin -scale 10"
        },
        {
            "name": "generic launch for hw_emu flows",
            "target": "hw_emu",
            "cmd_args": " -xclbin BUILD/semi_join.xclbin -scale 5000"
        },
        {
            "name": "generic launch for hw flows",
            "target": "hw",
            "cmd_args": " -xclbin BUILD/semi_join.xclbin"
        }
    ],
    "host": {
        "host_exe": "test_join.exe",
        "compiler": {
            "sources": [
                "host/test_join.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "host",
                "LIB_DIR/ext/xcl2",
                "LIB_DIR/../utils/L1/include",
                "kernel"
            ],
            "options": "-O3 ",
            "symbols": [
                "USE_HBM"
            ]
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw",
                "kernel",
                "host"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/hashjoinkernel.cpp",
                    "frequency": 300,
                    "name": "join_kernel"
                }
            ],
            "frequency": 300,
            "name": "semi_join"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": {
                    "vitis_hw_build": 40960,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 470,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}
