{
    "name": "Xilinx gqeJoin Kernel Bloom Filter Test",
    "description": "Xilinx gqeJoin kernel run as Bloom filter",
    "flow": "vitis",
    "gui": true,
    "platform_type": "pcie",
    "platform_whitelist": [
        "xilinx_u280_xdma_201920_3",
        "xilinx_u50_gen3x16_xdma_201920_3"
    ],
    "platform_blacklist": [
        "other"
    ],
    "platform_properties": {
        "u280": {
            "containers": [
                {
                    "name": "gqe_filter",
                    "ldclflags": "--config PROJECT/conn_u280.cfg --config PROJECT/opts.cfg --vivado.prop run.impl_1.STEPS.OPT_DESIGN.TCL.POST=PROJECT/post_opt.tcl"
                }
            ]
        },
        "u50": {
            "containers": [
                {
                    "name": "gqe_filter",
                    "ldclflags": "--config PROJECT/conn_u50.cfg --config PROJECT/opts.cfg --vivado.prop run.impl_1.STEPS.OPT_DESIGN.TCL.POST=PROJECT/post_opt.tcl"
                }
            ]
        }
    },
    "launch": [
        {
            "name": "generic launch for sw_emu flows",
            "target": "sw_emu",
            "cmd_args": " -xclbin BUILD/gqe_filter.xclbin -in db_data/ -scale 10000"
        },
        {
            "name": "generic launch for hw_emu flows",
            "target": "hw_emu",
            "cmd_args": " -xclbin BUILD/gqe_filter.xclbin -in db_data/ -scale 100000"
        },
        {
            "name": "generic launch for hw flows",
            "target": "hw",
            "cmd_args": " -xclbin BUILD/gqe_filter.xclbin -in db_data/ -scale 100"
        }
    ],
    "host": {
        "host_exe": "test_gqeJoin_bloomfilter.exe",
        "compiler": {
            "sources": [
                "host/test_q5s.cpp",
                "LIB_DIR/L2/tests/common/xclhost.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L1/include/hw",
                "LIB_DIR/L1/include/sw",
                "LIB_DIR/L2/include/sw",
                "LIB_DIR/L2/tests/common",
                "host"
            ],
            "options": "-g -O3 -D XDEVICE=$(XDEVICE)"
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw",
                "LIB_DIR/L2/include/hw",
                "LIB_DIR/../utils/L1/include"
            ],
            "clflags": [
                "-g --advanced.param compiler.fsanitize=address,memory"
            ],
            "hls_jobs": 16
        },
        "linker": {
            "synth_jobs": 16,
            "impl_jobs": 16
        }
    },
    "containers": [
        {
            "name": "gqe_filter",
            "accelerators": [
                {
                    "name": "gqeJoin",
                    "location": "LIB_DIR/L2/src/hw/gqe_kernel_join_filter.cpp",
                    "frequency": 250,
                    "num_compute_units": 1
                }
            ],
            "frequency": 180
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": {
                    "vitis_hw_build": 81920,
                    "vitis_hw_emu": 28672,
                    "vitis_sw_emu": 10240,
                    "vitis_hw_run": 10240
                },
                "max_time_min": {
                    "vitis_hw_build": 2400,
                    "vitis_hw_emu": 470,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}
