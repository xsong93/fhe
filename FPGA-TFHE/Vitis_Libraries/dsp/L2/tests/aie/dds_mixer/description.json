{
    "name": "Xilinx DSPLIB dds_mixer",
    "description": "",
    "gui": "false",
    "flow": "versal",
    "platform_whitelist": [
        "vck190"
    ],
    "platform_properties": {
        "vck190": {
            "v++": {
                "compiler": {
                    "clflags": []
                },
                "linker": {
                    "ldclflags": [
                        "--config PROJECT/system.cfg"
                    ]
                }
            }
        }
    },
    "pre_build": [
        {
            "build_cmd": "tclsh LIB_DIR/L2/tests/aie/common/scripts/gen_input.tcl $(LOC_INPUT_FILE) $(WINDOW_VSIZE) $(NITER_UUT) $(DATA_SEED) $(DATA_STIM_TYPE)"
        },
        {
            "build_cmd": "TARGET=x86sim UUT_KERNEL=dds_mixer_ref UUT_SIM_FILE=./data/ref_output.txt make x86sim TARGET=x86sim TAG=REF"
        },
        {
            "build_cmd": "make cleanall"
        }
    ],
    "post_launch": [
        {
            "target": "aiesim",
            "launch_cmd": [
                "mkdir -p logs",
                "grep -ve '[XT]' ./aiesimulator_output/data/uut_output.txt > ./logs/uut_output.txt",
                "grep -ve '[XT]' ./x86simulator_output/data/ref_output.txt > ./logs/ref_output.txt",
                "tclsh LIB_DIR/L2/tests/aie/common/scripts/diff.tcl ./logs/uut_output.txt ./logs/ref_output.txt ./logs/diff.txt $(DIFF_TOLERANCE) abs",
                "tclsh LIB_DIR/L2/tests/aie/common/scripts/get_common_config.tcl $(STATUS_FILE) ./ UUT_KERNEL $(UUT_KERNEL) DATA_TYPE $(DATA_TYPE) WINDOW_VSIZE $(WINDOW_VSIZE) MIXER_MODE $(MIXER_MODE)",
                "tclsh LIB_DIR/L2/tests/aie/common/scripts/get_stats.tcl $(WINDOW_VSIZE) 1 $(STATUS_FILE) ./aiesimulator_output dds $(NITER_UUT)",
                "tclsh LIB_DIR/L2/tests/aie/common/scripts/theoretical_minimum_scripts/get_dds_theoretical_min.tcl $(DATA_TYPE) $(MIXER_MODE) $(WINDOW_VSIZE) $(STATUS_FILE) $(UUT_KERNEL)",
                "LIB_DIR/L2/tests/aie/common/scripts/harvest_memory.sh $(STATUS_FILE) LIB_DIR/L2/tests/aie/common/scripts",
                "tclsh LIB_DIR/L2/tests/aie/common/scripts/diff_exit.tcl ./"
                ]
        },
        {
            "target": "x86sim",
            "launch_cmd": [
                "mkdir -p logs",
                "grep -ve '[XT]' ./x86simulator_output/data/uut_output.txt > ./logs/uut_output.txt",
                "grep -ve '[XT]' ./x86simulator_output/data/ref_output.txt > ./logs/ref_output.txt",
                "tclsh LIB_DIR/L2/tests/aie/common/scripts/diff.tcl ./logs/uut_output.txt ./logs/ref_output.txt ./logs/diff.txt $(DIFF_TOLERANCE) abs"
                ]
        }
    ],
    "launch": [
        {
            "name": "aiesim-launch",
            "target": "aiesim",
            "pkg-dir": "./Work",
            "input-dir": "./"
        },
        {
            "name": "x86sim-launch",
            "target": "x86sim",
            "pkg-dir": "./Work",
            "input-dir": "./"
        }
    ],
    "aiecompiler": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/aie",
                "LIB_DIR/L1/src/aie",
                "LIB_DIR/L1/tests/aie/inc",
                "LIB_DIR/L1/tests/aie/src",
                "LIB_DIR/L2/include/aie",
                "LIB_DIR/L2/tests/aie/common/inc",
                "./"
            ],
            "customized_params": {
                "params" : "--dataflow -v  $($(TAG)_TARGET_COMPILE_ARGS) --test-iterations=$(NITER_$(TAG)) -workdir=$($(TAG)_WORK_DIR) -Xpreproc=\"$(TAG_PREPROC_ARGS)\"",
                "default": {
                    "TAG": "UUT",
                    "UUT_KERNEL": "dds_mixer",
                    "REF_KERNEL": "dds_mixer_ref",
                    "UUT_GRAPH": "dds_mixer_graph",
                    "REF_GRAPH": "dds_mixer_ref_graph",
                    "NITER":"16",
                    "NITER_UUT":"$(NITER)",
                    "NITER_REF":"$$(($(NITER_UUT) / 2))",
                    "MIXER_MODE":"0",
                    "WINDOW_VSIZE":"256",
                    "P_API":"0",
                    "UUT_WINDOW_VSIZE":"$(WINDOW_VSIZE)",
                    "REF_WINDOW_VSIZE": "$$(( $(UUT_WINDOW_VSIZE) * $(NITER_UUT) / $(NITER_REF)))",
                    "DATA_TYPE":"cint16",
                    "DIFF_TOLERANCE": "4",
                    "DATA_SEED": "1",
                    "DATA_STIM_TYPE": "0",
                    "UUT_SIM_FILE":"./data/uut_output.txt",
                    "REF_SIM_FILE": "./data/ref_output.txt",
                    "UUT_SIM_FILE2":"./data/uut_output_2.txt",
                    "REF_SIM_FILE2": "./data/ref_output_2.txt",
                    "INPUT_FILE": "data/input.txt",
                    "LOC_INPUT_FILE":"./data/input.txt",
                    "UUT_FILE_SUFFIX": "$(UUT_KERNEL)_$(DATA_TYPE)_$(MIXER_MODE)_$(WINDOW_VSIZE)",
                    "STATUS_FILE": "./logs/status_$(UUT_FILE_SUFFIX).txt",
                    "UUT_TARGET_COMPILE_ARGS": "-Xchess=llvm.xargs=\"-std=c++2a\" -Xchess=main:backend.mist2.xargs=\"+NOdra\" -Xchess=main:noodle.optim.olbb=20 -Xchess=main:backend.mist2.pnll=\"off\" ",
                    "REF_TARGET_COMPILE_ARGS":"",
                    "UUT_WORK_DIR":"./Work",
                    "REF_WORK_DIR":"./Work",
                    "PREPROC_ARGS": "-DINPUT_FILE=$(LOC_INPUT_FILE) -DINPUT_FILE2=$(LOC_INPUT_FILE) -DDATA_TYPE=$(DATA_TYPE) -DMIXER_MODE=$(MIXER_MODE) -DP_API=$(P_API) ",
                    "USING_REF": "",
                    "USING_UUT": "-DUSING_UUT=1",
                    "TAG_PREPROC_ARGS": "-DUUT_GRAPH=$($(TAG)_GRAPH) -DOUTPUT_FILE=$($(TAG)_SIM_FILE) -DOUTPUT_FILE2=$($(TAG)_SIM_FILE2) $(PREPROC_ARGS) -DINPUT_WINDOW_VSIZE=$($(TAG)_WINDOW_VSIZE) -DNITER=$(NITER_$(TAG)) $(USING_$(TAG))"
                }
            }
        }
    },
    "aiecontainers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/aie/dds_mixer/test.cpp",
                    "pl-freq": 1000,
                    "workdir": "./Work",
                    "options": "--use-phy-shim=false",
                    "name": "libadf.a"
                }
            ]
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": {
                    "vitis_aie_x86sim": 60,
                    "vitis_aie_sim": 200
                }
            }
        ],
        "customized_params": "multi_params.json",
        "targets": [
            "vitis_aie_sim",
            "vitis_aie_x86sim"
        ],
        "category": "canary"
    }
}
