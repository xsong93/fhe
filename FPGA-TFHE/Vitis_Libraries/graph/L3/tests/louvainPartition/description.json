{
    "gui": false, 
    "name": "Xilinx Louvain L3 partition Test", 
    "description": "With L3 partition API for louvain, we could firstly partition a huge graph to some sub-graph with suitable size for cu processing, then launch cu on board Multi-thread concurrently and merge the result of louvain", 
    "flow": "vitis", 
    "platform_whitelist": [
        "u55c"
    ], 
    "platform_blacklist": [
    ], 
    "platform_properties": {
        "u55c": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_U55C"
                    ]
                }
            }, 
	        "v++": {
	            "compiler": {
		        "cflags": [
		            "--config PROJECT/conn_u55c.cfg --advanced.param compiler.userPostSysLinkOverlayTcl=PROJECT/postSysLink.tcl"
		            ]
		        }
	        }
	    }
    }, 
    "data": [
        "./data"
    ],
    "launch": [
        {
            "cmd_args": " ./data/example-wt.txt -kernel_mode 2 -num_pars 2 -create_alveo_BFS_partitions -name example_tx", 
            "ld_library_path": "PROJECT/../../lib:$(XILINX_XRT)/lib:$(XILINX_XRM)/lib", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "LIB_DIR/L3/tests/louvainPartition/test_partition.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L3/include"
            ], 
            "options": "-O3 -I$(XILINX_XRM)/include -DPRAGMA -Ofast -fopenmp -fPIC -DMULTITHREAD -DNDEBUG -B/usr/lib/x86_64-linux-gnu"
        },
        "linker": {
            "librarypaths": [
                "LIB_DIR/L3/lib"
            ], 
            "libraries" : [
                "graphL3"
            ],
            "options": "-L$(XILINX_XRM)/lib -lxrm -lgomp "
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include", 
                "LIB_DIR/../utils/L1/include", 
                "LIB_DIR/../database/L1/include/hw", 
                "LIB_DIR/../quantitative_finance/L1/include", 
                "LIB_DIR/L2/tests/louvianfast/kernel"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {

                    "location": "LIB_DIR/L2/tests/louvain_fast/kernel/kernel_louvain.cpp", 
                    "frequency": 250.0, 
                    "clflags": " -D KERNEL_NAME=kernel_louvain", 
                    "name": "kernel_louvain",
                    "num_compute_units": 1,
                    "compute_units": [
                    {
                        "name": "kernel_louvain",
                        "slr": "SLR0",
                        "arguments": [
                            {
                                "name": "m_axi_gmem0", 
                                "memory": "HBM[4]"
                            }, 
                            {
                                "name": "m_axi_gmem1", 
                                "memory": "HBM[0:1]"
                            }, 
                            {
                                "name": "m_axi_gmem2", 
                                "memory": "HBM[2:3]"
                            }, 
                            {
                                "name": "m_axi_gmem3", 
                                "memory": "HBM[5:6]"
                            }, 
                            {
                                "name": "m_axi_gmem4", 
                                "memory": "HBM[5:6]"
                            }, 
                            {
                                "name": "m_axi_gmem5", 
                                "memory": "HBM[7]"
                            }, 
                            {
                                "name": "m_axi_gmem6", 
                                "memory": "HBM[8]"
                            }, 
                            {
                                "name": "m_axi_gmem7", 
                                "memory": "HBM[9]"
                            },
                            {
                                "name": "m_axi_gmem8", 
                                "memory": "HBM[10]"
                            },
                            {
                                "name": "m_axi_gmem9", 
                                "memory": "HBM[11]"
                            },
                            {
                                "name": "m_axi_gmem10", 
                                "memory": "HBM[12]"
                            },
                            {
                                "name": "m_axi_gmem11", 
                                "memory": "HBM[13]"
                            },
                            {
                                "name": "m_axi_gmem12", 
                                "memory": "HBM[14]"
                            },
                            {
                                "name": "m_axi_gmem13", 
                                "memory": "HBM[15]"
                            },
                            {
                                "name": "m_axi_gmem14", 
                                "memory": "HBM[4]"
                            },
                            {
                                "name": "m_axi_gmem15", 
                                "memory": "HBM[0:1]"
                            }
                        ]
                    }
                    ]
                }
            ], 

            "frequency": 250.0, 
            "name": "kernel_louvain"
        }
    ], 
    "testinfo": {
        "disable": true, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}
