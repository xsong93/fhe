{
    "name": "Xilinx Quadrature Engine Used In Heston Closed Form Model", 
    "description": [
        "L2 Quadrature and HCF engine"
    ], 
    "flow": "vitis", 
    "platform_whitelist": [
        "u250", 
        "u50", 
        "u200"
    ], 
    "platform_blacklist": [], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 960, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }, 
    "host": {
        "compiler": {
            "sources": [
                "LIB_DIR/ext/xcl2/xcl2.cpp", 
                "./src/host/quad_hcf_test.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L2/demos/Quadrature/src/kernel", 
                "LIB_DIR/L2/include/", 
                "LIB_DIR/L1/include/"
            ]
        }
    }, 
    "containers": [
        {
            "name": "quad_hcf_kernel", 
            "accelerators": [
                {
                    "name": "quad_hcf_kernel", 
                    "location": "src/kernel/quad_hcf_kernel_wrapper.cpp", 
                    "files": [
                        "src/kernel/quad_hcf_engine.cpp", 
                        "src/kernel/quad_integrate_pi1.cpp", 
                        "src/kernel/quad_integrate_pi2.cpp"
                    ]
                }
            ]
        }
    ], 
    "launch": [
        {
            "name": "generic launch for all flows", 
            "cmd_args": "BUILD/quad_hcf_kernel.xclbin"
        }
    ], 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include/"
            ]
        }
    }
}
