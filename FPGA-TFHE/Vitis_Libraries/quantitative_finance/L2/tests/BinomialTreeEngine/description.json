{
    "name": "Xilinx Binomial Tree", 
    "description": [
        "L2 Binomial Tree"
    ], 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u250", 
        "u200", 
        "u50"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 720, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }, 
    "host": {
        "compiler": {
            "sources": [
                "LIB_DIR/ext/xcl2/xcl2.cpp", 
                "./src/host/binomialtreecpu.cpp", 
                "./src/host/binomialtreehost.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L2/include", 
                "./src/kernel", 
                "LIB_DIR/ext/xcl2/"
            ], 
            "symbols": [
                "TEST_DT=float", 
                "TEST_PARALLEL_ENGINES=8"
            ]
        }
    }, 
    "v++": {
        "compiler": {
            "symbols": [
                "TEST_DT=float", 
                "TEST_PARALLEL_ENGINES=8"
            ], 
            "includepaths": [
                "LIB_DIR/L2/include"
            ]
        }
    }, 
    "containers": [
        {
            "name": "binomialtree", 
            "accelerators": [
                {
                    "name": "BinomialTreeKernel", 
                    "location": "src/kernel/binomialtreekernel.cpp"
                }
            ]
        }
    ], 
    "launch": [
        {
            "name": "generic launch for all flows", 
            "cmd_args": "-xclbin BUILD/binomialtree.xclbin -data PROJECT/data"
        }
    ]
}
