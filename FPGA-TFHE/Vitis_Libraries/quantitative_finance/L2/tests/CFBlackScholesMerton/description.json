{
    "name": "Xilinx  Closed Form Black Scholes Merton Model", 
    "description": "L2 Closed Form Black Scholes Merton Model", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u250", 
        "u200", 
        "u50"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u200": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u200.cfg"
                    ]
                }
            }
        }, 
        "u250": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u250.cfg"
                    ]
                }
            }
        }, 
        "u50": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_DDR"
                    ]
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u50.cfg"
                    ]
                }
            }
        }
    }, 
    "launch": [
        {
            "cmd_args": " BUILD/bsm_kernel.xclbin 16384", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "bsm_test.exe", 
        "compiler": {
            "sources": [
                "LIB_DIR/L2/tests/CFBlackScholesMerton/src/host/bsm_test.cpp", 
                "LIB_DIR/L2/tests/CFBlackScholesMerton/src/host/bsm_model.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L2/include", 
                "LIB_DIR/ext/xcl2/"
            ], 
            "options": "-O3 "
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/CFBlackScholesMerton/src/kernel/bsm_kernel.cpp", 
                    "frequency": 300.0, 
                    "name": "bsm_kernel"
                }
            ], 
            "frequency": 300.0, 
            "name": "bsm_kernel"
        }
    ], 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/include"
            ]
        }
    }, 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}