{
    "name": "Xilinx Credit Default Swap Engine", 
    "description": "L2 Credit Default Swap", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u250", 
        "u200", 
        "u50"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u200": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u200.cfg"
                    ]
                }
            }
        }, 
        "u250": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u250.cfg"
                    ]
                }
            }
        }, 
        "u50": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u50.cfg"
                    ]
                }
            }
        }
    }, 
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/cds_engine.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "cds_host.exe", 
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/tests/CreditDefaultSwapEngine/src/kernel", 
                "LIB_DIR/L2/include", 
                "LIB_DIR/L1/include", 
                "LIB_DIR/ext/xcl2"
            ], 
            "sources": [
                "LIB_DIR/L2/tests/CreditDefaultSwapEngine/src/host/main.cpp", 
                "LIB_DIR/L2/tests/CreditDefaultSwapEngine/src/host/cpu.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "options": "-O3 ", 
            "symbols": [
                "TEST_DT=float"
            ]
        }
    }, 
    "v++": {
        "compiler": {
            "symbols": [
                "TEST_DT=float"
            ], 
            "includepaths": [
                "LIB_DIR/L1/include", 
                "LIB_DIR/L2/include"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/tests/CreditDefaultSwapEngine/src/kernel/CDS_kernel.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D TEST_DT=float", 
                    "name": "CDS_kernel"
                }
            ], 
            "frequency": 300.0, 
            "name": "cds_engine"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}