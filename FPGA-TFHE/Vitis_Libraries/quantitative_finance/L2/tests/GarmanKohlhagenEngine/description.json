{
    "name": "Xilinx Garman Kohlhagen Model", 
    "description": [
        "L2 Garman Kohlhagen Model"
    ], 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u250", 
        "u50", 
        "u200"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u200": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u200.cfg"
                    ]
                }
            }
        }, 
        "u250": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u250.cfg"
                    ]
                }
            }
        }, 
        "u50": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u50.cfg"
                    ]
                }
            }
        }
    },
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }, 
    "host": {
        "compiler": {
            "sources": [
                "LIB_DIR/ext/xcl2/xcl2.cpp", 
                "./src/host/gk_test.cpp", 
                "./src/host/gk_parser.cpp", 
                "./src/host/gk_cpu.cpp"
            ]
        }
    }, 
    "containers": [
        {
            "name": "gk_kernel", 
            "accelerators": [
                {
                    "name": "gk_kernel", 
                    "location": "src/kernel/gk_kernel.cpp"
                }
            ]
        }
    ], 
    "launch": [
        {
            "name": "generic launch for all flows", 
            "cmd_args": "BUILD/gk_kernel.xclbin PROJECT/one.txt"
        }
    ]
}
