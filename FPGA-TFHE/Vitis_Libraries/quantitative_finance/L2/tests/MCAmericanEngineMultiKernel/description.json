{
    "gui": true, 
    "name": "Xilinx MC American Option Pricing Engine With MultiKernel Test", 
    "description": "The American option can be exercised at any time up until its maturity date. This early exercise feature of American Option gives the investors more freedom to exercise, compared to the European options.", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u250", 
        "u200", 
        "u50"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u200": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": ["--config PROJECT/conn_u200.cfg"]
                }
            }, 
            "containers": [
                {
                    "ldclflags_mk": "--connectivity.nk MCAE_k0:1:MCAE_k0_1 --connectivity.nk MCAE_k1:1:MCAE_k1_1 --connectivity.nk MCAE_k2:1:MCAE_k2_1", 
                    "frequency": 300, 
                    "name": "MCAE_k"
                }
            ]
        }, 
        "u250": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": ["--config PROJECT/conn_u250.cfg"]
                }
            }, 
            "containers": [
                {
                    "ldclflags_mk": "--connectivity.nk MCAE_k0:1:MCAE_k0_1 --connectivity.nk MCAE_k1:1:MCAE_k1_1 --connectivity.nk MCAE_k2:1:MCAE_k2_1", 
                    "frequency": 300, 
                    "name": "MCAE_k"
                }
            ]
        }, 
        "u50": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": ["--config PROJECT/conn_u50.cfg"]
                }
            }, 
            "containers": [
                {
                    "ldclflags_mk": "--connectivity.nk MCAE_k0:1:MCAE_k0_1 --connectivity.nk MCAE_k1:1:MCAE_k1_1 --connectivity.nk MCAE_k2:1:MCAE_k2_1", 
                    "frequency": 300, 
                    "name": "MCAE_k"
                }
            ]
        }
    }, 
    "launch": [
        {
            "cmd_args": " -mode fpga -xclbin BUILD/MCAE_k.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "host/main.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L1/include", 
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/tests/MCAmericanEngineMultiKernel/host", 
                "LIB_DIR/L2/tests/MCAmericanEngineMultiKernel/kernel", 
                "LIB_DIR/ext/xcl2"
            ], 
            "options": "-O3 ", 
            "symbols": []
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include", 
                "LIB_DIR/L2/include", 
                "LIB_DIR/L2/tests/MCAmericanEngineMultiKernel/kernel"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/MCAE_k0.cpp", 
                    "frequency": 300, 
                    "clflags": " -D DPRAGMA", 
                    "name": "MCAE_k0", 
                    "num_compute_units": 1
                }, 
                {
                    "location": "kernel/MCAE_k1.cpp", 
                    "frequency": 300, 
                    "clflags": " -D DPRAGMA", 
                    "name": "MCAE_k1", 
                    "num_compute_units": 1
                }, 
                {
                    "location": "kernel/MCAE_k2.cpp", 
                    "frequency": 300, 
                    "clflags": " -D DPRAGMA", 
                    "name": "MCAE_k2", 
                    "num_compute_units": 1
                }
            ], 
            "frequency": 300, 
            "name": "MCAE_k"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
