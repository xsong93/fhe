                    "vitis_hw_emu": 20480, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 960, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
